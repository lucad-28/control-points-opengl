{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 10
  },
  "configurePresets": [
    {
      "name": "x86-Debug",
      "hidden": false,
      "description": "x86 Debug build",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_C_COMPILER": "C:/MinGW64/bin/gcc.exe",
        "CMAKE_CXX_COMPILER": "C:/MinGW64/bin/g++.exe"
      }
    }
  ]
}
